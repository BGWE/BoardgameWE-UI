{
  "name": "boardgame-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "yarn watch-translations & ./node_modules/@vue/cli-service/bin/vue-cli-service.js serve",
    "build": "npm run translations2json && ./node_modules/@vue/cli-service/bin/vue-cli-service.js build",
    "lint": "vue-cli-service lint",
    "watch-translations": "./node_modules/onchange/cli.js -i 'src/locales/translations.csv' -- yarn translations2json",
    "translations2json": "./node_modules/i18n-csv2json-cli/bin/main.js --from src/locales/translations.csv --to src/locales/json --format"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "axios": "^0.21.1",
    "buefy": "^0.8.8",
    "lodash.debounce": "^4.0.8",
    "moment-timezone": "^0.5.23",
    "qs": "^6.7.0",
    "socket.io-client": "^2.2.0",
    "vee-validate": "^3.3.0",
    "verte": "^0.0.12",
    "vue": "^2.5.22",
    "vue-color": "^2.7.0",
    "vue-fab": "^2.3.1",
    "vue-i18n": "^8.8.2",
    "vue-infinite-loading": "^2.4.5",
    "vue-moment": "^4.1.0",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.2",
    "vue-socket.io-extended": "^4.0.1",
    "vue-youtube": "^1.3.4",
    "vuedraggable": "^2.20.0",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.0.1",
    "i18n-csv2json-cli": "^1.0.2",
    "node-sass": "^4.13.1",
    "onchange": "^6.1.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^3.0.0 || ^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
